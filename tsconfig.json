{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "ES3",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": false,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "baseUrl": "."
  }
}
